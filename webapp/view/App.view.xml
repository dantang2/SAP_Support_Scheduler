<mvc:View xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:layout="sap.ui.layout" xmlns:form="sap.ui.layout.form" xmlns:mvc="sap.ui.core.mvc" controllerName="productSupport.scheduler.controller.App" displayBlock="true">
	<App class="myAppDemoWT">
		<pages>
			<Page title="Product Support Scheduler">
				<headerContent class="sapUiSmallMargin">
					<Select width="auto" maxWidth="100%" icon="" enabled="true" type="Default" textAlign="Center" valueState="None" forceSelection="false">
						<items>
							<core:Item key="ADMIN" text="ADMIN" enabled="true"/>
							<core:Item key="WEBI" text="WEBI" enabled="true"/>
						</items>
					</Select>
				</headerContent>
				<content>
					<Button id="scheduleButton" text="Schedule" press="onOpenAppointmentDialog" class="sapUiSmallMargin" icon="sap-icon://check-availability"/>
					<Button id ="addEngineerButton" icon="sap-icon://add-employee" class="sapUiSmallMargin" press="onOpenDialog"/>
					<mvc:XMLView viewName="productSupport.scheduler.view.EngineerList"/>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>
