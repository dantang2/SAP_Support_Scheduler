<mvc:View xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" controllerName="productSupport.scheduler.controller.App" displayBlock="true">
	<App class="myAppDemoWT">
		<pages>
			<Page title="Product Support Scheduler">
				<headerContent>
					<Button icon="sap-icon://add-employee" press="onOpenDialog"/>
				</headerContent>
				<content>
					<Button id="scheduleButton" text="Schedule" press="onSchedule" class="sapUiSmallMarginEnd" icon="sap-icon://check-availability"/>
					<mvc:XMLView viewName="productSupport.scheduler.view.HelloPanel"/>
					<mvc:XMLView viewName="productSupport.scheduler.view.EngineerList"/>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>
